{
	"type": "object",
	"required": [
		"firstName",
		"lastName",
		"email"
	],
	"properties": {
		"firstName": {
			"type": "string",
			"title": "First name"
		},
		"lastName": {
			"type": "string",
			"title": "Last name"
		},
		"address": {
			"type": "string",
			"title": "Street Address"
		},
		"city": {
			"type": "string",
			"title": "City"
		},
		"state": {
			"type": "string",
			"enum": ["AK", "AL", "AR", "AZ", "CA", "CO", "CT", "DE", "FL", "GA", "HI", "IA", "ID", "IL", "IN", "KS", "KY", "LA", "MA", "MD", "ME", "MI", "MN", "MO", "MS", "MT", "NC", "ND", "NE", "NH", "NJ", "NM", "NV", "NY", "OH", "OK", "OR", "PA", "RI", "SC", "SD", "TN", "TX", "UT", "VA", "VT", "WA", "WI", "WV", "WY"],
			"title": "State"
		},
		"zip": {
			"type": "string",
			"title": "Zip Code"
		},
		"email": {
			"type": "string",
			"title": "Email"
		},
		"homeScene": {
			"type": "string",
			"title": "Home Scene"
		},
		"weekendPassType": {
			"type": "string",
			"enum": ["Full Weekend Pass", "Dance Only Pass ($45.00)"],
			"title": "Weekend Pass Type"
		},
		"student": {
			"type": "boolean",
			"title": "I am a student"
		},
		"workshopLevel": {
			"type": "string",
			"enum": ["Level 1", "Level 2", "Level 3"],
			"title": "Workshop Level"
		},
		"mixAndMatch": {
			"type": "boolean",
			"title": "Mix and Match Competition ($5.00)"
		},
		"mixAndMatchRole": {
			"type": "string",
			"enum": ["Follower", "Leader"],
			"title": "Mix and Match Role"
		},
		"soloJazz": {
			"type": "boolean",
			"title": "Solo Jazz Competition ($5.00)"
		},
		"teamCompetition": {
			"type": "boolean",
			"title": "Team Competition ($55.00)"
		},
		"teamName": {
			"type": "string",
			"title": "Team Name"
		},
		"tShirt": {
			"type": "boolean",
			"title": "T-Shirt"
		},
		"tShirtSize": {
			"type": "string",
			"enum": ["Unisex S", "Unisex M", "Unisex L", "Unisex XL", "Unisex 2XL", "Unisex 3XL", "Bella S", "Bella M", "Bella L", "Bella XL", "Bella 2XL"],
			"title": "T-Shirt Size"
		},
		"HousingStatus": {
			"type": "string",
			"enum": ["I require housing", "I can provide housing"],
			"title": "Housing accommodation"
		},
		"myPets": {
			"type": "string",
			"title": "I have the following pets (cats, dogs, etc)"
		},
		"housingNumber": {
			"type": "integer",
			"title": "I can house this many people"
		},
		"myHousingDetails": {
			"type": "string",
			"title": "Any other details the organizers should know about my house"
		},
		"petAllergies": {
			"type": "string",
			"title": "I am allergic to the following pets"
		},
		"housingRequestDetails": {
			"type": "string",
			"title": "Anything else I would like to say about my housing request"
		},
		"coupon": {
			"type": "string",
			"title": "Coupon code"
		}
	}
